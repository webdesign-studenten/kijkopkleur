	<event name="customer_address_save_after">
		<observer instance="WebdesignStudenten\EasySync\Observer\Customer\AddressSaveAfter" name="webdesignstudenten_easysync_observer_customer_addresssaveafter_customer_address_save_after"/>
	</event>
	<event name="catalog_category_save_after">
    <observer instance="WebdesignStudenten\EasySync\Observer\Category\CategorySaveAfter" name="webdesignstudenten_easysync_observer_category_save"/>
  </event>
	<event name="catalog_product_save_after">
		<observer instance="WebdesignStudenten\EasySync\Observer\Catalog\ProductSaveAfter" name="webdesignstudenten_easysync_observer_catalog_productsaveafter_catalog_product_save_after"/>
	</event>
    <event name="newsletter_subscriber_save_after">
        <observer name="webdesignstudenten_easysync_observer_newsletter_saveafterdataobject" instance="WebdesignStudenten\EasySync\Observer\Newsletter\SaveAfterDataObject" />
