<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
	<event name="checkout_cart_add_product_complete">
		<observer instance="WebdesignStudenten\EasySync\Observer\Sales\CheckoutCartAddProductCompleteObserver" name="webdesignstudenten_easysync_observer_checkout_cart_add_product_complete_observer"/>
	</event>
	<event name="sales_order_place_after">
		<observer instance="WebdesignStudenten\EasySync\Observer\Sales\OrderPlaceAfter" name="webdesignstudenten_easysync_observer_sales_orderplaceafter_sales_order_place_after"/>
	</event>
	<event name="sales_order_invoice_save_after">
		<observer instance="WebdesignStudenten\EasySync\Observer\Sales\OrderInvoiceSaveAfter" name="webdesignstudenten_easysync_observer_sales_orderinvoicesaveafter"/>
	</event>
	<event name="sales_order_shipment_save_after">
		<observer instance="WebdesignStudenten\EasySync\Observer\Sales\OrderShipmentSaveAfter" name="webdesignstudenten_easysync_observer_sales_ordershipmentsaveafter"/>
	</event>
	<event name="wishlist_add_product">
		<observer instance="WebdesignStudenten\EasySync\Observer\Wishlist\AddProduct" name="webdesignstudenten_easysync_observer_wishlist_addproduct_wishlist_add_product"/>
	</event>
	<event name="customer_save_after_data_object">
		<observer instance="WebdesignStudenten\EasySync\Observer\Customer\SaveAfterDataObject" name="webdesignstudenten_easysync_observer_customer_saveafterdataobject_customer_save_after_data_object"/>
